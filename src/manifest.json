{
  "manifest_version": 3,
  "name": "Codespace Web Tweaks",
  "description": "Codespace Web Tweaks",
  "version": "0.1",
  "icons": {
    "128": "icon-128.png",
    "48": "icon-48.png",
    "16": "icon-16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.github.dev/"
      ],
      "js": [
        "codespaces.js"
      ],
      "run_at": "document_end"
    }
  ]
}
